{
  "name": "zeronaut-contracts",
  "version": "1.0.0",
  "description": "Contracts for Zeronaut",
  "main": "src/index.js",
  "scripts": {
    "postinstall": "npm run build",
    "build": "cd test/fixture-projects/basic-level && npm i",
    "compile": "npx hardhat compile && bash scripts/compile-fixtures.sh",
    "test": "npm run compile && mocha",
    "generate:contract": "bash scripts/generate-contract.sh",
    "generate:acir": "bash scripts/generate-acir.sh",
    "deploy": "hardhat ignition deploy ignition/modules/Zeronaut.js --network localhost",
    "deploy:hard": "hardhat ignition deploy ignition/modules/Zeronaut.js --network localhost --reset"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@noir-lang/backend_barretenberg": "0.31.0",
    "@noir-lang/noir_js": "0.31.0",
    "@nomicfoundation/hardhat-toolbox": "^5.0.0",
    "hardhat": "^2.22.11",
    "mocha": "^10.7.3"
  }
}
